class NhanvienListComponent {
    constructor(nhanVienService, router, message) {
        this.nhanVienService = nhanVienService;
        this.router = router;
        this.message = message;
        this.pageTitle = 'Danh sách nhân viên';
        this.dataSource = [];
        this.isConfirmLoading = false;
    }
    ngOnInit() {
        this.isConfirmLoading = true;
        this.loadNhanVienList();
    }
    // get danh sach nhan vien
    loadNhanVienList() {
        this.nhanVienService.getAll().subscribe(response => {
            if (response && response.Status) {
                this.dataSource = response.Data;
                this.isConfirmLoading = false;
                // console.log(this.dataSource);
            }
        });
    }
    // xem chi tiết nhân viên
    viewNhanvien(manhanvien) {
        this.navigateDetail(manhanvien);
    }
    navigateDetail(manhanvien) {
        if (manhanvien) {
            // chuyen sang màn hình chi tiết nhan vien
            this.router.navigate(['nhanvien-management/detail/'], { queryParams: { manhanvien: manhanvien } });
        }
        else {
            this.message.create(src_app_app_constant__WEBPACK_IMPORTED_MODULE_1__["TypeMessage"].Error, 'Có lỗi xảy ra!');
        }
    }
}
